                    <param name="bgcolor" value="#FFFFFF"/>
                    <param name="flashVars" value="@videoPlayer=2523145419001&amp;playerID=1656678413001&amp;playerKey=AQ~~,AAABfJkxEHE~,20THWYuqpxwTOQB5inJOAWk-Yy2gcGI9&amp;domain=embed&amp;dynamicStreaming=true"/>
