                                <td><a href="http://www.restaurantcater.asn.au/" onclick="ntptEventTag('ev=PartnerLead&amp;PartnerId=savour');" target="_blank"><img alt="" src="/contentimages/Campaigns/campaigns-tqual-logo-savour.jpg" /></a></td>
                                <td><a href="http://www3.yha.com.au/" onclick="ntptEventTag('ev=PartnerLead&amp;PartnerId=yha');" target="_blank"><img alt="" src="/contentimages/Campaigns/campaigns-tqual-logo-yha.jpg" /></a></td>
                                <td></td>
