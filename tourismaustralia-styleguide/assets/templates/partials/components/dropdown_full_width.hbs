---
title: DropDown Full Width
---

<div class="dropdown-full-width">
    <div class="dropdown-full-width-style">
        <select>
            {{#if data}}
                {{#each data}}
                    <option>{{option}}</option>
                {{/each}}
            {{else}}
                <option>Option 1</option>
                <option>Option 2</option>
            {{/if}}
        </select>
    </div>
</div>
