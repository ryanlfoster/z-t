---
title: DropDown White Background
---

<div class="dropdown-white-background">
    <div class="dropdown-white-background-style">
        <select>
            {{#if data}}
                {{#each data}}
                    <option>{{option}}</option>
                {{/each}}
            {{else}}
                <option>Option 1</option>
                <option>Option 2</option>
            {{/if}}
        </select>
    </div>
</div>
