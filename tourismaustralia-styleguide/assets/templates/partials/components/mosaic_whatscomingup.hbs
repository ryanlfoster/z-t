---
title: Mosaic Standard - Whats Coming Up
---

{{#if captions}}
    <div class="mosaic mosaic-with-captions">
{{else}}
    <div class="mosaic">
{{/if}}

    <div class="row l-row-collapse">

        <div class="row">

            {{#if captions}}
                {{#parseJSON '{"captions": true}'}}
                    {{> mosaic_whatscomingup_grid_1 }}
                {{/parseJSON}}
            {{else}}
                {{> mosaic_whatscomingup_grid_1 }}
            {{/if}}

            {{#if captions}}
                {{#parseJSON '{"captions": true}'}}
                    {{> mosaic_whatscomingup_grid_4 }}
                {{/parseJSON}}
            {{else}}
                {{> mosaic_whatscomingup_grid_4 }}
            {{/if}}

        </div>

        <div class="row">

            {{#if captions}}
                {{#parseJSON '{"captions": true}'}}
                    {{> mosaic_whatscomingup_grid_4 }}
                {{/parseJSON}}
            {{else}}
                {{> mosaic_whatscomingup_grid_4 }}
            {{/if}}

            {{#if captions}}
                {{#parseJSON '{"captions": true}'}}
                    {{> mosaic_whatscomingup_grid_1 }}
                {{/parseJSON}}
            {{else}}
                {{> mosaic_whatscomingup_grid_1 }}
            {{/if}}

        </div>

    </div>
</div>


